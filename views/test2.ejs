<style>
    html {
        height: 100%;
    }
    
    body {
        /* to centre page on screen*/
        margin-left: auto;
        margin-right: auto;
        font-family: 'Lato' !important;
        padding: 10px;
        height: 100%;
    }
    
    *,
    *::after {
        box-sizing: border-box;
        margin: 0;
    }
    
    body {
        color: #454f54;
        background-color: #f4f5f6;
        background-image: linear-gradient(to bottom left, #abb5ba, #d5dadd);
    }
    
    .ticket {
        display: flex;
        flex-direction: column;
        max-width: 55rem;
        border: 6px solid #9d8c7a;
        padding: 5px;
        margin: 0 auto;
        height: 365px;
        position: relative;
        width: 100%;
    }
    
    .inner-border {
        border: 3px solid #9d8c7a;
        position: relative;
    }
    
    .text-center {
        text-align: center;
    }
    
    .row {
        display: flex;
        flex-wrap: wrap;
    }
    
    .col {
        flex-basis: 0;
        flex-grow: 1;
        max-width: 100%;
    }
    
    .col-6 {
        flex: 0 0 50%;
        max-width: 50%;
    }
    
    .col-8 {
        flex: 0 0 66.666667%;
        max-width: 66.666667%;
    }
    
    .ticket__header,
    .ticket__body,
    .ticket__footer {
        padding: 1.25rem;
        background-color: white;
        border: 1px solid #abb5ba;
        box-shadow: 0 2px 4px rgba(41, 54, 61, 0.25);
    }
    
    .ticket__left_body {
        padding: 10px;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        height: 100%;
        color: black;
        background: repeating-linear-gradient( 130deg, transparent, transparent 15px, #eae1d8 10px, #eae1d8 34px), linear-gradient(to bottom, #f8f5ee, #f5f2eb);
    }
    
    .mb-3 {
        margin-bottom: 1rem;
    }
    
    .mb-2 {
        margin-bottom: 1rem;
    }
    
    .hr-text {
        line-height: 1em;
        position: relative;
        outline: 0;
        border: 0;
        color: black;
        text-align: center;
        height: 1.5em;
        opacity: 0.5;
    }
    
    .hr-text:before {
        content: "";
        background: linear-gradient(to right, transparent, #818078, transparent);
        position: absolute;
        left: 0;
        top: 50%;
        width: 100%;
        height: 1px;
    }
    
    .hr-text:after {
        content: attr(data-content);
        position: relative;
        display: inline-block;
        color: black;
        padding: 0 0.5em;
        line-height: 1.5em;
        color: #818078;
        background-color: #fcfcfa;
    }
    
    .text-label {
        color: #9d8e7b;
        background-color: #eae1d8;
        padding: 4px;
    }
    
    .text-input {
        font-weight: 600;
        color: #686666;
    }
    
    .ticket__header {
        font-size: 1.5rem;
        border-bottom: none;
        box-shadow: none;
        padding: 0 5px;
        text-align: center;
    }
    
    .ticket__wrapper {
        box-shadow: 0 2px 4px rgba(41, 54, 61, 0.25);
        border-radius: 0.375em 0.375em 0 0;
        overflow: hidden;
    }
    
    .ticket__divider {
        position: relative;
        height: 1rem;
        background-color: white;
        margin-left: 0.5rem;
        margin-right: 0.5rem;
    }
    
    .ticket__divider::after {
        content: "";
        position: absolute;
        height: 50%;
        width: 100%;
        top: 0;
        border-bottom: 2px dashed #e9ebed;
    }
    
    .ticket__notch {
        position: absolute;
        left: -0.5rem;
        width: 1rem;
        height: 1rem;
        overflow: hidden;
    }
    
    .ticket__notch::after {
        content: "";
        position: relative;
        display: block;
        width: 2rem;
        height: 2rem;
        right: 100%;
        top: -50%;
        border: 0.5rem solid white;
        border-radius: 50%;
        box-shadow: inset 0 2px 4px rgba(41, 54, 61, 0.25);
    }
    
    .ticket__notch--right {
        left: auto;
        right: -0.5rem;
    }
    
    .ticket__notch--right::after {
        right: 0;
    }
    
    .ticket__body {
        border-bottom: none;
        border-top: none;
    }
    
    .ticket__body>*+* {
        margin-top: 1.5rem;
        padding-top: 1.5rem;
        border-top: 1px solid #e9ebed;
    }
    
    .ticket__section>*+* {
        margin-top: 0.25rem;
    }
    
    .ticket__section>h3 {
        font-size: 1.125rem;
        margin-bottom: 0.5rem;
    }
    
    .CircleBox {
        width: 70px;
        height: 120px;
        display: block;
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        overflow: hidden;
    }
    
    .CircleBox span {
        width: 120px;
        height: 120px;
        border: 5px solid #9D8C7A;
        position: absolute;
        border-radius: 50%;
        background-color: #fff;
    }
    
    .CircleBox.CircleLeft {
        left: -6px;
    }
    
    .CircleBox.CircleLeft span {
        right: 0;
    }
    
    .CircleBox.CircleRight {
        right: -6px;
    }
    
    .CircleBox.CircleRight span {
        left: 0;
    }
</style>

